{
  "name": "runes-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "json_server": "json-server --watch ./db/base.json -r ./db/routes.json -m ./node_modules/json-server-auth --port 3004",
    "serve": "tsc && vite build && json-server --watch ./db/base.json -r ./db/routes.json -m ./node_modules/json-server-auth --port 3004 --static ./dist ",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "i18next": "^22.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-i18next": "^12.0.0",
    "react-router-dom": "^6.4.4",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/ui": "^0.25.3",
    "json-server": "^0.17.1",
    "json-server-auth": "^2.1.0",
    "sass": "^1.56.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.3"
  }
}